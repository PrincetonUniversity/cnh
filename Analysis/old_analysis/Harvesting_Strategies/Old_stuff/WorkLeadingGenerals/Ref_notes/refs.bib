	pubmedid = {20810442},
	PMCID = {PMC3025682},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Olsen/Spawning%20stock%20and%20recruitment%20in%20North%20Sea%20cod%20sh.pdf,Sente,},
	url = {},
	keywords = {Gadus morhua;Animals;Fisheries;Food Web;Abiotic;North Sea;Conservation of Natural Resources;Species interactions;Population Dynamics;Feeding Behavior;Models, Theoretical;Fishing impacts;Zooplankton;Climate},
	tags = {Fishing impacts;Food Web;Abiotic;Species interactions},
	Citation_identifier = {Olsen 2011},
	abstract = {In order to provide better fisheries management and conservation decisions, there is a need to discern the underlying relationship between the spawning stock and recruitment of marine fishes, a relationship which is influenced by the environmental conditions. Here, we demonstrate how the environmental conditions (temperature and the food availability for fish larvae) influence the stock-recruitment relationship and indeed what kind of stock-recruitment relationship we might see under different environmental conditions. Using unique zooplankton data from the Continuous Plankton Recorder, we find that food availability (i.e. zooplankton) in essence determines which model applies for the once large North Sea cod (Gadus morhua) stock. Further, we show that recruitment is strengthened during cold years and weakened during warm years. Our combined model explained 45 per cent of the total variance in cod recruitment, while the traditional Ricker and Beverton-Holt models only explained about 10 per cent. Specifically, our approach predicts that a full recovery of the North Sea cod stock might not be expected until the environment becomes more favourable}
}
@article{Shackell:ProcBiolSci:2010,
	author = {Shackell, Nancy L.  and Frank, Kenneth T.  and Fisher, Jonathan A. D.  and Petrie, Brian  and Leggett, William C.},
	title = {Decline in top predator body size and changing climate alter trophic structure in an oceanic ecosystem.},
	journal = {Proc Biol Sci},
	uuid = {6FF24576-B05B-434F-B92D-23289CA8AA36},
	volume = {277},
	number = {1686},
	pages = {1353-60},
	organization = {Ocean Sciences Division, Bedford Institute of Oceanography, Dartmouth, Nova Scotia, Canada. nancy.shackell\@dfo-mpo.gc.ca},
	address = {England},
	month = {5},
	year = {2010},
	doi = {10.1098/rspb.2009.1020},
	ISSN = {1471-2954},
	US_NLM_ID = {101245157},
	PII = {rspb.2009.1020},
	pubmedid = {20031989},
	PMCID = {PMC2871930},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Shackell/Decline%20in%20top%20predator%20body%20size%20and%20changing%20cli.pdf,Sente,},
	url = {},
	keywords = {research support, non-u.s. gov't;Ecosystem;Animals;Oceans and Seas;Phytoplankton;Food Web;Biomass;Species Specificity;Predatory Behavior;Temperature;Species interactions;Gadiformes;Body Size;Fishing impacts;Zooplankton;Climate},
	tags = {Fishing impacts;Food Web;Species interactions},
	Citation_identifier = {Shackell 2010},
	abstract = {Globally, overfishing large-bodied groundfish populations has resulted in substantial increases in their prey populations. Where it has been examined, the effects of overfishing have cascaded down the food chain. In an intensively fished area on the western Scotian Shelf, Northwest Atlantic, the biomass of prey species increased exponentially (doubling time of 11 years) even though the aggregate biomass of their predators remained stable over 38 years. Concomitant reductions in herbivorous zooplankton and increases in phytoplankton were also evident. This anomalous trophic pattern led us to examine how declines in predator body size (approx. 60\% in body mass since the early 1970s) and climatic regime influenced lower trophic levels. The increase in prey biomass was associated primarily with declines in predator body size and secondarily to an increase in stratification. Sea surface temperature and predator biomass had no influence. A regression model explained 65 per cent of prey biomass variability. Trait-mediated effects, namely a reduction in predator size, resulted in a weakening of top predation pressure. Increased stratification may have enhanced growing conditions for prey fish. Size-selective harvesting under changing climatic conditions initiated a trophic restructuring of the food chain, the effects of which may have influenced three trophic levels}
}
