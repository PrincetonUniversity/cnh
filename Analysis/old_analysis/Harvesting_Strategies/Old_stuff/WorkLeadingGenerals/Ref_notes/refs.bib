	url = {http://www.jstor.org/stable/1748241},
	keywords = {Food Web;Modeling;Marine;Management;Moving Fish;Modeling of fish harvest},
	tags = {Modeling;Management;Moving Fish;Modeling of fish harvest;Marine;Food Web},
	Copyright = {Copyright \copyright 1979 American Association for the Advancement of Science},
