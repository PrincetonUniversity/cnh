	author = {Fujita, R. M.  and Honey, K. T.  and Morris, A.  and Wilson, J. R.  and Russell, H.},
	title = {Cooperative strategies in fisheries management: integrating across scales},
	journal = {Bulletin of Marine Science},
	uuid = {08499555-CA86-4414-9CEA-1C5FE250253B},
	volume = {86},
