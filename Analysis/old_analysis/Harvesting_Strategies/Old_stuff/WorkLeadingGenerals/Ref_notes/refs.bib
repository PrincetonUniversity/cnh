	uuid = {5F4F65AD-87D0-42AA-8675-ADE7B001A9CF},
	volume = {108},
	number = {29},
	pages = {11942-7},
	organization = {Biology Department, Dalhousie University, Halifax, NS, Canada B3H 4R2. bworm\@dal.ca},
	address = {United States},
	month = {7},
	year = {2011},
	doi = {10.1073/pnas.1102353108},
	ISSN = {1091-6490},
	US_NLM_ID = {7505876},
	PII = {1102353108},
	pubmedid = {21693644},
	PMCID = {PMC3141942},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Worm/Range%20contraction%20in%20large%20pelagic%20predators.%202011.pdf,Sente,},
	url = {},
	keywords = {research support, non-u.s. gov't;Animals;Oceans and Seas;Fisheries;macroecology;Demography;Homing Behavior;Marine;Species Specificity;Perciformes;Conservation of Natural Resources;Population Dynamics;Fishing impacts;Marine Biology},
	tags = {Fishing impacts;macroecology;Marine},
	Citation_identifier = {Worm 2011},
	abstract = {Large reductions in the abundance of exploited land predators have led to significant range contractions for those species. This pattern can be formalized as the range-abundance relationship, a general macroecological pattern that has important implications for the conservation of threatened species. Here we ask whether similar responses may have occurred in highly mobile pelagic predators, specifically 13 species of tuna and billfish. We analyzed two multidecadal global data sets on the spatial distribution of catches and fishing effort targeting these species and compared these with available abundance time series from stock assessments. We calculated the effort needed to reliably detect the presence of a species and then computed observed range sizes in each decade from 1960 to 2000. Results suggest significant range contractions in 9 of the 13 species considered here (between 2\% and 46\% loss of observed range) and significant range expansions in two species (11-29\% increase). Species that have undergone the largest declines in abundance and are of particular conservation concern tended to show the largest range contractions. These include all three species of bluefin tuna and several marlin species. In contrast, skipjack tuna, which may have increased its abundance in the Pacific, has also expanded its range size. These results mirror patterns described for many land predators, despite considerable differences in habitat, mobility, and dispersal, and imply ecological extirpation of heavily exploited species across parts of their range},
	notes = {() Found that 9 out of the 13 billfish and tuna species examined had range declines. There is a non significant correlation with abundance, but not enough data exists to be sure. Used presence-absence data derived from catch data from Japanese long-lining and FAO catch data. 
From this data it's not possible to know what drives the range changes. It could be due to ocean conditions, removal by fisheries, species interactions or other factors.}
}
@article{Hunsicker:EcolLett:2011,
	author = {Hunsicker, Mary E.  and Ciannelli, Lorenzo  and Bailey, Kevin M.  and Buckel, Jeffrey A.  and Wilson White, J.  and Link, Jason S.  and Essington, Timothy E.  and Gaichas, Sarah  and Anderson, Todd W.  and Brodeur, Richard D.  and Chan, Kung-Sik S. and Chen, Kun  and Englund, G\"{o}ran  and Frank, Kenneth T.  and Freitas, V\^{a}nia  and Hixon, Mark A.  and Hurst, Thomas  and Johnson, Darren W.  and Kitchell, James F.  and Reese, Doug  and Rose, George A.  and Sjodin, Henrik  and Sydeman, William J.  and van der Veer, Henk W.  and Vollset, Knut  and Zador, Stephani},
	title = {Functional responses and scaling in predator-prey interactions of marine fishes: contemporary issues and emerging concepts.},
