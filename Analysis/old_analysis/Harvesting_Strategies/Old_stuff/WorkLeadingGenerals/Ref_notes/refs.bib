	uuid = {F10AA96B-25B4-4EC6-8B95-AD45833A1EF5},
	volume = {14},
	number = {12},
	pages = {1288-99},
	organization = {College of Oceanic and Atmospheric Sciences, Oregon State University, Corvallis, OR 97331, USA. mhunsicker\@coas.oregonstate.edu},
	address = {England},
	month = {12},
