	author = {Scheffer, Marten  and Bascompte, Jordi  and Brock, William A.  and Brovkin, Victor  and Carpenter, Stephen R.  and Dakos, Vasilis  and Held, Hermann  and van Nes, Egbert H.  and Rietkerk, Max  and Sugihara, George},
	title = {Early-warning signals for critical transitions.},
	journal = {Nature},
	uuid = {61564811-D9C6-4AE5-908A-263402DBDEFB},
	volume = {461},
	number = {7260},
	pages = {53-9},
	organization = {Department of Environmental Sciences, Wageningen University, PO Box 47, 6700 AA Wageningen, The Netherlands. marten.scheffer\@wur.nl},
	address = {England},
	month = {9},
	year = {2009},
	doi = {10.1038/nature08227},
	ISSN = {1476-4687},
	US_NLM_ID = {0410462},
	PII = {nature08227},
	pubmedid = {19727193},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Scheffer/Early-warning%20signals%20for%20critical%20transitions.%2020.pdf,Sente,},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Scheffer/Early-warning%20signals%20for%20critical%20transitions.%2020%20%232.pdf,Sente,},
	url = {},
	keywords = {research support, non-u.s. gov't;Ecosystem;research support, u.s. gov't, non-p.h.s.;Animals;Stochastic Processes;Humans;Models, Economic;Seizures;review;Models, Biological;Extinction, Biological;EEB 324;critical transitions;Asthma;Eutrophication;Climate},
	tags = {EEB 324;critical transitions},
	Citation_identifier = {Scheffer 2009},
	abstract = {Complex dynamical systems, ranging from ecosystems to financial markets and the climate, can have tipping points at which a sudden shift to a contrasting dynamical regime may occur. Although predicting such critical points before they are reached is extremely difficult, work in different scientific fields is now suggesting the existence of generic early-warning signals that may indicate for a wide class of systems if a critical threshold is approaching},
	notes = {() Many complex ssytems have critical transitions (tipping points) where the system shifts abruptly from one state to another. Increasing evidence that there are general symptoms that may appear in a wide range of systems before a critical transition.
For systems shifting from one stable equilibrium to another there there are:
 Critical slowing down: As the system gets closer to a catastrophic bifurcation, eigenvalue declines to zero, thus the speed at which the system responds declines with the magnitude of the eigenvalue. The result is that autocorrelation between time points inreases (as the system spends longer in each perturbation). Increased variance can also result as perturbations don't decay. Although the opposite could also result becasue now the state of the system may not track perturbations. But model analysis shows that increased variance shows up earlier. 
Can also get skewedness of perturbations because as the state approaches a catastrophic bifurcation, the alternate basin of attraction is closer. So the system may spend more time during perturbations there. This could also lead to flickering, where perturbations are able to push the system back and forth across the basins of attraction.
Critical transistions between cyclic and chaotic systems are less well studied, but can get critical slowing down for Hopf bifurcations (moving from single stable point to oscillatory system). BUt also such things as non-local bifurcations that occur when intrinsic oscillations bring system to border of basin of attraction of an alternate attractor. But such bifurcations are not associated with any particular properties or stable/unstable points that can be analytically defined. No explicit early warning signals for these, but may see dynamics change in characteristic way before basin-boundary conditions occur. Oscillations might become stretched where system resides in boundary where rates of change are slower (implying increased autocorrelation). 
Spatial patterns may also serve as warning. Vanishing scale-invariant power laws or tendancy towards increased spatial coherence (or in oscillating units, resonance). BZut these signals depend on system class, no one-size-fits-all. 
Empirical examples include: ancient climate shifts (Younger Dryas); ecosystems (rangelands, fisheries); and physiological (asthma attacks, epileptic seizures). 
Key issue is whether a signal can be detected early enough to do something about it. But the generality of symptoms should be cause for optimism.}
}
@inbook{Terborgh:TrophicCascadesPredatorsPreyAndTheChanging:2010,
	author = {Essington, Tim},
	editor = {Terborgh, John  and Estes, James A.},
	title = {Trophic Cascades in Open Ocean Ecosystems},
	booktitle = {Trophic Cascades: Predators, Prey, and the Changing Dynamics of Nature},
	uuid = {1A41D0CF-2EF6-40A8-A1EB-3DF522D2F4AF},
	pages = {91-105},
	publisher = {Island Press},
	year = {2010},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Terborgh/Trophic%20Cascades%20in%20Open%20Ocean%20Ecosystems%202010%20Tro.pdf,Sente,},
	url = {},
	keywords = {Food webs},
	tags = {Food webs},
	Citation_identifier = {Terborgh 2010},
	publicationStatus = {Unknown},
	notes = {() Historically the idea of consumer control in marine systems has been dismissed, and it was assumed that resources and geophysical characteristics were the drivers of ecosystem processes with the biological system as a passenger. This is beginning to change with increasing evidence of consumer control, but geophysical drivers remain important in understanding underlying systems. However many meta-analyses comparing strengths of trophic-cascades in different systems have found evidence for weaker trophic cascades in marine ecosystems than in other environments. This result could be caused by a number of mechanisms unique to marine habitats such as: high functional redundancy, complex life history of apex predators, and advection. 
Most apex predators are teleost fish, in which recruitment and availability of adult prey are partially decoupled. Thus recruitment (can?) functions as a partial energetic subsidy for adults (since availability of offspring to eat is not related to availability of adult prey). But this type of energetic subsidy has been shown to strengthen and stability top-down consumer control, rather than weaken it. 
Functional redundancy has also been shown to mediate the strength of trophic cascades. In a modeling exercise, reducing apex predators resulted in an increase in mesopredators, however there was no concomitant change in lower trophic levels. In this particular model this is due to the fact that mesopredators prey on a diverse assemblage of secondary consumers, and thus the impact of any one predator on the secondary consumers was small.  Unfortunately these models aggregate large number of species within a functional group, so it's impossible to determine within-guild dynamics. This is problematic because dramatic trophic cascades found in lake systems are due to size selectivity within a functional group. Thus the lack of resolution within a functional group precludes the ability to accurately model/understand how these within guild specificities might shape trophic interactions in marine systems.
Advection may also serve to limit trophic cascades. Upwelling systems often have high primary productivity, but these systems end up exporting much of the primary productivity offshore due to the water currents which promote upwelling in the first place. Thus consumption and production of primary productivity are spatially decoupled, limiting the ability of consumption to limit production. 
Based on these limitations, it's hypothesized that systems with low functional redundancy and little advection might show trophic cascades. And there is some evidence to support this idea. In semi-enclosed seas (where advection is lower) and on coastal shelves that do not have upwelling (where tidal action mixes the water column, examples are all in the Atlantic) there are various examples of consumer control. However even these examples are often highly context dependent. 
While there's not a plethora of evidence of strong trophic cascades, it's also possible that the lack of control areas and inability to experiment on appropriate spatial scales (i.e. huge) limit evidence from being collected. Because of the dearth of control areas, most studies depend on time-series, which have the problem of environmental drivers possibly being responsible for the changes in abundance. Further often many systems are being perturbed at multiple levels (from the top and middle by fishing, at the bottom by eutrophication), making it difficult to prove that top-down control is primarily responsible for changes in the marine community.}
}
