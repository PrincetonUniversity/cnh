@article{Wilsonetal:2007,
	author = {Wilson, James  and Yan, Liying  and Wilson, Carl},
	title = {The precursors of governance in the Maine lobster fishery},
	journal = {PNAS},
	uuid = {1E8FE344-1E8B-4E9B-91E0-2C5F75A9DC0D},
	volume = {104},
