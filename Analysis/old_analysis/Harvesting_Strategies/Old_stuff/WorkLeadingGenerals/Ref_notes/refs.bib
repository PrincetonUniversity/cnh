@article{Naidoo:ProcNatlAcadSciUSA:2008,
	author = {Naidoo, R.  and Balmford, A.  and Costanza, R.  and Fisher, B.  and Green, R. E.  and Lehner, B.  and Malcolm, T. R.  and Ricketts, T. H.},
	title = {Global mapping of ecosystem services and conservation priorities.},
	journal = {Proc Natl Acad Sci U S A},
