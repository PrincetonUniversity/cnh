@article{Ostrom:2010,
	author = {Ostrom, Elinor},
	title = {Beyond markets and states: polycentric governance of complex economic systems},
	journal = {American Economic Review},
	uuid = {34CCE9B3-BD56-4EAA-B167-520B20DCC1FA},
	volume = {100},
	number = {3},
