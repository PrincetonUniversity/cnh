@article{Boyd:Nature:2000,
	author = {Boyd, P. W.  and Watson, A. J.  and Law, C. S.  and Abraham, E. R.  and Trull, T.  and Murdoch, R.  and Bakker, D. C.  and Bowie, A. R.  and Buesseler, K. O.  and Chang, H.  and Charette, M.  and Croot, P.  and Downing, K.  and Frew, R.  and Gall, M.  and Hadfield, M.  and Hall, J.  and Harvey, M.  and Jameson, G.  and LaRoche, J.  and Liddicoat, M.  and Ling, R.  and Maldonado, M. T.  and McKay, R. M.  and Nodder, S.  and Pickmere, S.  and Pridmore, R.  and Rintoul, S.  and Safi, K.  and Sutton, P.  and Strzepek, R.  and Tanneberger, K.  and Turner, S.  and Waite, A.  and Zeldis, J.},
	title = {A mesoscale phytoplankton bloom in the polar Southern Ocean stimulated by iron fertilization.},
	journal = {Nature},
	uuid = {FE96656D-0645-440D-8562-A1D8048EA81F},
	volume = {407},
	number = {6805},
	pages = {695-702},
	organization = {National Institute of Water and Atmosphere, Centre for Chemical and Physical Oceanography, Department of Chemistry, University of Otago, Dunedin, New Zealand. pboyd\@alkali.otago.ac.nz},
	address = {ENGLAND},
	month = {10},
	year = {2000},
	doi = {10.1038/35037500},
	ISSN = {0028-0836},
	US_NLM_ID = {0410462},
	pubmedid = {11048709},
	sentelink = {file://localhost/Users/emmafuller/Documents/Sente/Emma%27s%20reading/Boyd/A%20mesoscale%20phytoplankton%20bloom%20in%20the%20polar%20South.pdf,Sente,},
	url = {},
	keywords = {Atmosphere;Forecasting;research support, non-u.s. gov't;Phytoplankton;Oceans and Seas;Seawater;Carbon Dioxide;Iron;Models, Biological;Time Factors;Abiotic;Eutrophication;Light;Fundamentals;Fertilizers;research support, u.s. gov't, non-p.h.s.},
	tags = {Fundamentals;Abiotic},
	Citation_identifier = {Boyd 2000},
	abstract = {Changes in iron supply to oceanic plankton are thought to have a significant effect on concentrations of atmospheric carbon dioxide by altering rates of carbon sequestration, a theory known as the 'iron hypothesis'. For this reason, it is important to understand the response of pelagic biota to increased iron supply. Here we report the results of a mesoscale iron fertilization experiment in the polar Southern Ocean, where the potential to sequester iron-elevated algal carbon is probably greatest. Increased iron supply led to elevated phytoplankton biomass and rates of photosynthesis in surface waters, causing a large drawdown of carbon dioxide and macronutrients, and elevated dimethyl sulphide levels after 13 days. This drawdown was mostly due to the proliferation of diatom stocks. But downward export of biogenic carbon was not increased. Moreover, satellite observations of this massive bloom 30 days later, suggest that a sufficient proportion of the added iron was retained in surface waters. Our findings demonstrate that iron supply controls phytoplankton growth and community composition during summer in these polar Southern Ocean waters, but the fate of algal carbon remains unknown and depends on the interplay between the processes controlling export, remineralisation and timescales of water mass subduction}
}
@article{Coale:Science:2004,
	author = {Coale, Kenneth H.  and Johnson, Kenneth S.  and Chavez, Francisco P.  and Buesseler, Ken O.  and Barber, Richard T.  and Brzezinski, Mark A.  and Cochlan, William P.  and Millero, Frank J.  and Falkowski, Paul G.  and Bauer, James E.  and Wanninkhof, Rik H.  and Kudela, Raphael M.  and Altabet, Mark A.  and Hales, Burke E.  and Takahashi, Taro  and Landry, Michael R.  and Bidigare, Robert R.  and Wang, Xiujun  and Chase, Zanna  and Strutton, Pete G.  and Friederich, Gernot E.  and Gorbunov, Maxim Y.  and Lance, Veronica P.  and Hilting, Anna K.  and Hiscock, Michael R.  and Demarest, Mark  and Hiscock, William T.  and Sullivan, Kevin F.  and Tanner, Sara J.  and Gordon, R. Mike  and Hunter, Craig N.  and Elrod, Virginia A.  and Fitzwater, Steve E.  and Jones, Janice L.  and Tozzi, Sasha  and Koblizek, Michal  and Roberts, Alice E.  and Herndon, Julian  and Brewster, Jodi  and Ladizinsky, Nicolas  and Smith, Geoffrey  and Cooper, David  and Timothy, David  and Brown, Susan L.  and Selph, Karen E.  and Sheridan, Cecelia C.  and Twining, Benjamin S.  and Johnson, Zackary I.},
	title = {Southern Ocean iron enrichment experiment: carbon cycling in high- and low-Si waters.},
	journal = {Science},
	uuid = {4AAA1016-3CA9-4124-8AD5-5C360C8FAD41},
	volume = {304},
	number = {5669},
	pages = {408-14},
	organization = {Moss Landing Marine Laboratories, 8272 Moss Landing Road, Moss Landing, CA 95039-9647, USA. coale\@mlml.calstate.edu},
	address = {United States},
	month = {4},
